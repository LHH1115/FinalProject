<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="com/example/demo/db/db.properties"/>
	<typeAliases>
		<typeAlias type="com.example.demo.vo.MemberVO" alias="memberVO"/>
		<typeAlias type="com.example.demo.vo.EventVO" alias="eventVO"/>
		<typeAlias type="com.example.demo.vo.ReservationVO" alias="reservationVO"/>
		<typeAlias type="com.example.demo.vo.AccommodationVO" alias="accommodationVO"/>
		<typeAlias type="com.example.demo.vo.ReviewVO" alias="reviewVO"/>
		<typeAlias type="com.example.demo.vo.LikeVO" alias="likeVO"/>
		<typeAlias type="com.example.demo.vo.AccommoPhotoVO" alias="accommoPhotoVO"/>
	</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/MemberMapper.xml"/>
    <mapper resource="com/example/demo/db/MyPageMapper.xml"/>
  </mappers>
</configuration>